@model DD.Models.User

<h2>Đăng Ký Người Dùng</h2>

@using (Html.BeginForm())
{
    <div class="form-group">
        @Html.LabelFor(model => model.TenKH)
        @Html.TextBoxFor(model => model.TenKH, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.Email)
        @Html.TextBoxFor(model => model.Email, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.ThangSinh)
        @Html.TextBoxFor(model => model.ThangSinh, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.GioiTinh)
        @Html.TextBoxFor(model => model.GioiTinh, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.DiaChi)
        @Html.TextBoxFor(model => model.DiaChi, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.SoDienThoai)
        @Html.TextBoxFor(model => model.SoDienThoai, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.TenDangNhap)
        @Html.TextBoxFor(model => model.TenDangNhap, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.MatKhau)
        @Html.PasswordFor(model => model.MatKhau, new { @class = "form-control" })
    </div>

    <input type="submit" value="Đăng Ký" class="btn btn-primary" />
}